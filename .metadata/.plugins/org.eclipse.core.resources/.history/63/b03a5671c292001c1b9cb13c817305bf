public class Hello {
	public static void main (String[] args) 
	{
		
		int a = 10;
		int b = 20;
		
		System.out.println("Oi, meu chapa!");
		
		if(a>b) 
		{
			System.out.println("E lá vamos nós!");
			
		}
		
		else 
		{
			System.out.println("Se o pica-pau tivesse informado à polícia, nada disso teria acontecido! ");
		}
		
		if((a==10)&&(b==20)) 
		{
			System.out.println("Delicia gelada!\nHummm...... Goxtoso!");
		}
		
	}
	
} 